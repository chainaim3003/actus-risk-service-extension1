          "raw": "{\"contracts\":[{\"contractType\":\"SWPPV\",\"contractID\":\"SWPPV-PC-MX-autoparts-01\",\"statusDate\":\"2026-02-28T00:00:00\",\"contractRole\":\"RF\",\"currency\":\"USD\",\"contractDealDate\":\"2026-02-28T00:00:00\",\"initialExchangeDate\":\"2026-03-01T00:00:00\",\"maturityDate\":\"2026-05-31T00:00:00\",\"notionalPrincipal\":6000000,\"cycleAnchorDateOfInterestPayment\":\"2026-03-31T00:00:00\",\"cycleOfInterestPayment\":\"P1ML1\",\"nominalInterestRate\":0.055,\"nominalInterestRate2\":0.043,\"dayCountConvention\":\"A365\",\"cycleAnchorDateOfRateReset\":\"2026-03-01T00:00:00\",\"rateSpread\":0.015,\"marketObjectCodeOfRateReset\":\"USD_SOFR\",\"fixingPeriod\":\"P0D\",\"deliverySettlement\":\"D\",\"tariffModels\":[\"pc_swppv_MX_01\"]}],\"scenarioDescriptor\":{\"scenarioID\":\"swppv_pc_MX_scn01\",\"scenarioType\":\"scenario\"},\"simulateTo\":\"2026-05-31T00:00:00\",\"monitoringTimes\":[]}",
          "options": {
